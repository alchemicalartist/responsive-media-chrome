{
  "name": "@alchemicalartist/responsive-media-chrome",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "ssh:git@github.com/alchemicalartist/responsive-media-chrome.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "lint": "eslint --cache --ext .ts,.js,.mjs,.cjs . && prettier -c src",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs . --fix && prettier -c src -w",
    "prepack": "npm run build",
    "release": "&& changelogen --release && npm publish && git push --follow-tags",
    "test": "npm lint && vitest run --coverage"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "^0.32.2",
    "changelogen": "^0.5.3",
    "eslint": "^8.43.0",
    "eslint-config-unjs": "^0.2.1",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3",
    "unbuild": "^1.2.1",
    "vitest": "^0.32.2"
  },
  "dependencies": {
    "media-chrome": "^1.1.0"
  }
}
